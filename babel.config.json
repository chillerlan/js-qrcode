{
	"exclude": "node_modules/**",
	"plugins": [
		"@babel/plugin-syntax-import-attributes",
		"istanbul"
	],
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"last 2 versions",
						"ie >= 11"
					]
				},
				"useBuiltIns": "entry",
				"corejs": "3.37"
			}
		]
	]
}
